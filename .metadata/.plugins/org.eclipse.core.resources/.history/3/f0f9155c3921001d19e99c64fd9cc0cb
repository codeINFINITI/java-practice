	}

	public static void rec(Integer num) {
		if (num > 0) {
			System.out.println(num);
			rec(num - 1);
		}
	}
	
}
class Vehicle{
	public void drive() {
		System.out.println("vehicle");
	}
}
class Scooty extends Vehicle{
	public void drive() {
		System.out.println("Scooty");
	}
}
class ABC extends Scooty{
	public void drive() {
		System.out.println("ABC");
	}
}
